{
  "expo": {
    "name": "storylines",
    "slug": "storylines",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sl_icon_1024.png",
    "scheme": "storylines",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/sl_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imisshtml.storylines",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sl_icon_1024.png",
        "backgroundColor": "#050908"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.imisshtml.storylines"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sl_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "image": "./assets/images/sl_logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4846990712111779~8324709931",
          "iosAppId": "ca-app-pub-4846990712111779~3524358138"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "output": "server"
    }
  }
}
