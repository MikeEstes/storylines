{
  "expo": {
    "name": "storylines",
    "slug": "storylines",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sl_icon_1024.png",
    "scheme": "storylines",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sl_icon_1024.png",
        "backgroundColor": "#050908"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/storylines_splash.jpg",
          "imageWidth": 1024,
          "resizeMode": "cover",
          "backgroundColor": "#050908"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-4846990712111779~8324709931",
          "ios_app_id": "ca-app-pub-4846990712111779~3524358138"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "output": "server"
    }
  }
}